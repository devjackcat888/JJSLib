# platform :ios, '9.0'

def base_depency

  use_frameworks!
  # 屏蔽所有第三方框架警告
  inhibit_all_warnings!

   pod 'SnapKit'
   pod 'RxSwift'
   pod 'Closures'
   pod 'RxCocoa'
#   pod 'UIColor_Hex_Swift'
   pod 'Kingfisher'
   pod 'CryptoSwift'
   pod 'SwiftyRSA'
   pod 'Alamofire'
   pod 'SDWebImage'
   
   pod 'YYText'
#   pod 'GumboHTMLTransform', :git => 'https://gitee.com/devjackcat/gumbo-htmltransform.git'

  pod 'WCDB.swift'
  pod 'MJRefresh'
  pod 'KeychainAccess'
  
end

target 'JJSLib' do
  base_depency
end

target 'Example' do
  base_depency
end
